    "@jest/globals": "^29.7.0",
    "@types/node": "^22.6.1",
    "@types/ws": "^8.5.12",
    "electron": "^32.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@damienvesper/bit-buffer": "^1.0.1",
    "nanotimer": "^0.3.15",
    "ws": "^8.18.0"
  },
